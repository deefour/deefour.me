{
  "name": "deefour.me",
  "version": "5.0.0",
  "description": "My website.",
  "main": "src/index.pug",
  "scripts": {
    "clean": "yarn del-cli {dist,.cache}",
    "build": "yarn parcel build src/index.pug",
    "watch": "yarn parcel src/index.pug",
    "format": "yarn prettier --write '{./*,./src/**/*}.{js,ts,css,json}'",
    "check": "yarn tsc --noEmit --project .",
    "lint": "yarn check && yarn tslint -c tslint.json './**/*.ts' -t stylish"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/deefour/deefour.me.git"
  },
  "keywords": [
    "deefour"
  ],
  "author": "Jason Daly <jason@deefour.me>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/deefour/deefour.me/issues"
  },
  "homepage": "https://deefour.me",
  "devDependencies": {
    "autoprefixer": "^9.7.3",
    "del-cli": "^3.0.0",
    "parcel-bundler": "^1.12.4",
    "prettier": "^1.19.1",
    "pug": "^2.0.4",
    "sass": "^1.24.0",
    "tslint": "^5.20.1",
    "typescript": "^3.7.3"
  },
  "dependencies": {
    "babel-polyfill": "^6.26.0",
    "normalize.css": "^8.0.1"
  }
}
