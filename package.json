{
  "name": "deefour.me",
  "version": "5.0.0",
  "description": "My website.",
  "main": "src/index.pug",
  "scripts": {
    "clean": "yarn del-cli {dist,.cache}",
    "build": "yarn parcel build src/index.pug",
    "watch": "yarn parcel src/index.pug",
    "format": "yarn prettier --write '{./*,./src/**/*}.{js,ts,scss,css,json}'",
    "check": "yarn tsc --noEmit --project .",
    "lint": "yarn check && yarn eslint . --ext .ts && yarn stylelint '**/*.scss' -s scss"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/deefour/deefour.me.git"
  },
  "keywords": [
    "deefour"
  ],
  "author": "Jason Daly <jason@deefour.me>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/deefour/deefour.me/issues"
  },
  "homepage": "https://deefour.me",
  "devDependencies": {
    "@babel/core": "^7.8.4",
    "@typescript-eslint/eslint-plugin": "^2.21.0",
    "@typescript-eslint/parser": "^2.21.0",
    "autoprefixer": "^9.7.4",
    "del-cli": "^3.0.0",
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^6.10.0",
    "husky": "^4.2.3",
    "parcel-bundler": "^1.12.4",
    "prettier": "^1.19.1",
    "pretty-quick": "^2.0.1",
    "pug": "^2.0.4",
    "sass": "^1.26.1",
    "stylelint": "^13.2.0",
    "stylelint-scss": "^3.14.2",
    "tslint": "^6.0.0",
    "typescript": "^3.8.2"
  },
  "dependencies": {
    "babel-polyfill": "^6.26.0",
    "lodash.debounce": "^4.0.8",
    "normalize.css": "^8.0.1"
  }
}
